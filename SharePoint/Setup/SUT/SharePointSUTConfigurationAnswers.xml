<!--This file provides configuration values to the setup configuration script in case the script runs in unattended mode. 
    Run the setup configuration script with one argument with the same path of the current XML file when the script runs in unattended mode. -->
	<!-- Note: Property values are case-insensitive unless otherwise specified. -->
<Properties>
  <!-- The runOnNonRecommendedSUT property specifies whether to run the SUT setup script when the SharePoint server is not the recommended version. 
		   The supported values are:
		   "1", run the SUT setup script;
		   "2", exit the SUT setup script;-->
  <Property name="runOnNonRecommendedSUT" value="1" />
	<!-- The password property specifies the password of the current logon user on SUT. The property value is case-sensitive. -->
	<Property name="password" value="Password01!" />
	<!-- The webAppName property specifies the name of the SharePoint site. This property is only used if the default SharePoint site "SharePoint - 80" does not exist. -->
	<Property name="webAppName" value="SharePoint - 81" />
</Properties>

